    "python.testing.pytestArgs": [
        "."
    ],
    "python.testing.unittestEnabled": true,
    "python.testing.pytestEnabled": false,
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "#002e00",
        "background": "#ffffff2b"
    },
